#!/bin/bash
cflags=$2                                                                       
cflags="${cflags%\"}"                                                           
cflags="${cflags#\"}"
cd src_corpora/FreeRDP

CC=$1 CFLAGS="$cflags" cmake -DCMAKE_BUILD_TYPE=Release
make
install -D libfreerdp/libfreerdp2.so.2.0.0 ../../$4/
mv ../../$4/libfreerdp2.so.2.0.0 ../../$4/libfreerdp2.$3
make clean
cd ../..
